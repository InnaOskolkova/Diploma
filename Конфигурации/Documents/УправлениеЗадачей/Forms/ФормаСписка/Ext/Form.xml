	ДокументУправлениеЗадачей.Исполнитель,
	ДокументУправлениеЗадачей.Статус,
	ДокументУправлениеЗадачей.ДополнительнаяИнформация,
	ДокументУправлениеЗадачей.ДатаФинала,
	ДокументУправлениеЗадачей.ПроцентВыполнения,
	ДокументУправлениеЗадачей.ПроцентПодготовкиТЗ,
	ДокументУправлениеЗадачей.КомментарийПоЗадаче,
	ВЫБОР
		КОГДА ДокументУправлениеЗадачей.РаботыВыполнены
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ДокументУправлениеЗадачей.ДатаФинала = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
					ТОГДА 1
				ИНАЧЕ ВЫБОР
						КОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, ДокументУправлениеЗадачей.ДатаФинала, ДЕНЬ) &lt;= 1
							ТОГДА 2
						ИНАЧЕ ВЫБОР
								КОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, ДокументУправлениеЗадачей.ДатаФинала, ДЕНЬ) &lt;= 7
									ТОГДА 3
								ИНАЧЕ 4
							КОНЕЦ
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ КАК Просрочка,
	ДокументУправлениеЗадачей.ЧасыДоФинала,
	ДокументУправлениеЗадачей.РаботыВыполнены,
	ДокументУправлениеЗадачей.ДатаТЗ,
	ДокументУправлениеЗадачей.ТРЗ,
	ДокументУправлениеЗадачей.ДатаПереносаВПродуктив,
	ДокументУправлениеЗадачей.Автор,
	ДокументУправлениеЗадачей.ДатаДО,
	ДокументУправлениеЗадачей.ПроцентПодготовкиДО,
	ДокументУправлениеЗадачей.Координатор
ИЗ
	Документ.УправлениеЗадачей КАК ДокументУправлениеЗадачей</QueryText>
