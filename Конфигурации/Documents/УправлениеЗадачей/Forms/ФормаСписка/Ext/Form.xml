				<QueryText>ВЫБРАТЬ
	ДокументУправлениеЗадачей.Ссылка,
	ДокументУправлениеЗадачей.ПометкаУдаления,
	ДокументУправлениеЗадачей.Номер,
	ДокументУправлениеЗадачей.Дата,
